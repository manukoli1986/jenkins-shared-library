node ('master'){
    
    stage('Clone'){
        git credentialsId: 'GithubCred', url: 'https://github.com/manukoli1986/simple-java-maven-app.git'
    }
    
    stage('Build'){
        input 'Choose branch'
        withMaven(maven: 'maven') {
            sh "mvn clean verify -DSkiptest=True"
        }
    }
    
    
}
